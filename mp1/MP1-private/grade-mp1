	'thread 1: 1',
	'thread 2: 1',
	'thread 3: 1',
	'thread 1: 2',
	'handler 1: 1',
	'thread 3: 2',
	'thread 1: 3',
	'handler 1: 2',
	'handler 1: 1',
	'thread 1: 4',
	'handler 1: 3',
	'thread 4: 0',
	'handler 1: 2',
	'thread 4: 1',
	'handler 1: 3',
	'thread 6: 0',
	'thread 6: 1',
	'',
	'exited'
    )

@test(10, "thread package with public testcase part2-6")
def test_thread_0():
    r.run_qemu(shell_script([
        'mp1-part2-6',
    ]))
    r.match(
