	' (-) ',
	' (o) ',
	' (o) ',
	' (.) ',
	' (-) ',
	' (-) ',
	' (-) ',
	' (o) ',
	' (.) ',
	' (.) ',
	' (-) ',
	' (.) ',
	' (-) ',
	' (-) ',
	' (.) ',
	' (.) ',
	' (.) ',
	' (-) ',
	' (.) ',
	' / \\ ',
	'',
	'exited',
    )

